#: import os os
<ScreenOne>:
    BoxLayout:
        canvas:
            Color:
                rgba: 0.95, 0.95, 0.95, 1
            Rectangle:
                pos: self.pos
                size: self.size
        spacing: 10
        padding: 50
        orientation: "vertical"
        Image:
            source: "wyp.png"
            size_hint:(1.0, 0.70)
        Button:
            background_normal: ""
            background_color: 0.55, 0.35, 0.20, 1
            text: "Upload Photo"
            color: 0.95, 0.95, 0.95, 1
            font_size: 18
            bold: True
            size_hint:(1.0, 0.15)
            on_press:
                root.manager.transition.direction = 'left'
                root.manager.current = 'screen_two'
        Button:
            background_normal: ""
            background_color: 0.55, 0.35, 0.20, 1
            text: "Take Quiz"
            color: 0.95, 0.95, 0.95, 1
            font_size: 18
            bold: True
            size_hint:(1.0, 0.15)
            on_press:
                root.manager.transition.direction = 'left'
                root.manager.current = 'screen_three'

<ScreenTwo>:
    BoxLayout:
        orientation: "vertical"
        FileChooserListView:
        Button:
            background_normal: ""
            background_color: 0.55, 0.35, 0.20, 1
            text: "Back"
            color: 0.95, 0.95, 0.95, 1
            font_size: 18
            bold: True
            size_hint:(1.0, 0.15)
            on_press:
                root.manager.transition.direction = 'right'
                root.manager.current = 'screen_one'

