<ScreenOne>:
    BoxLayout:
        canvas:
            Color:
                rgba: 0.95, 0.95, 0.95, 1
            Rectangle:
                pos: self.pos
                size: self.size
        spacing: 10
        padding: 50
        orientation: "vertical"
        Image:
            source: "wyp.png"
            size_hint:(1.0, 0.70)
